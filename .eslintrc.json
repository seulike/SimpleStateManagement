{
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "env": {
        "es6": true
    },
    "rules": {
        "semi": 0,
        "no-use-before-define":0,
        "comma-dangle": 0,
        "no-underscore-dangle": 0,
        "prefer-spread": 0
    },
    "plugins": [
        "flowtype"
    ]
}